                <constraints nullable="false"/>
            </column>
            <column name="updated_at" type="TIMESTAMP WITHOUT TIME ZONE">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="age" type="INTEGER"/>
            <column defaultValue="NULL::character varying" name="language" type="VARCHAR(255)"/>
            <column defaultValueBoolean="false" name="is_selfie_verified" type="BOOLEAN"/>
            <column defaultValue="NULL::character varying" name="user_verification_image" type="VARCHAR(255)"/>
            <column defaultValue="NULL::character varying" name="verification_token" type="VARCHAR(255)"/>
            <column name="shareable_link" type="TEXT"/>
            <column name="small_image_name" type="TEXT"/>
            <column name="direct_login_link" type="TEXT"/>
            <column defaultValueBoolean="false" name="is_self_deactivated" type="BOOLEAN"/>
            <column name="jetzy_exclusive_status" type="PERMISSION_STATUS"/>
            <column name="jetzy_select_status" type="PERMISSION_STATUS"/>
            <column name="user_level" type="USER_LEVEL"/>
            <column name="influencer_level" type="INFLUENCER_LEVEL"/>
        </createTable>
        <sql dbms="postgresql" splitStatements="true" stripComments="true">
            ALTER TABLE users ADD COLUMN IF NOT EXISTS effective_status effective_user_status GENERATED ALWAYS AS (
            CASE
            WHEN first_name IS NULL THEN 'pending'::effective_user_status
            WHEN first_name = '' THEN 'pending'::effective_user_status
            WHEN is_deleted THEN 'deleted'::effective_user_status
            WHEN is_deactivated OR is_self_deactivated THEN 'deactivated'::effective_user_status
            WHEN is_active THEN 'active'::effective_user_status
            ELSE 'pending'::effective_user_status
            END
            ) STORED;
        </sql>
    </changeSet>

