<script>
    $(document).on('change', '.btn-fileUpload :file', function () {
        var input = $(this),
            numFiles = input.get(0).files ? input.get(0).files.length : 1,
            label = input.val().replace(/\\/g, '/').replace(/.*\//, '');
        input.trigger('fileselect', [numFiles, label]);
    });

    $(document).ready(function () {
        $('.btn-fileUpload :file').on('fileselect', function (event, numFiles, label) {

            var input = $(this).parents('.input-group').find(':text'),
                log = numFiles > 1 ? numFiles + ' files selected' : label;

            if (input.length) {
                input.val(log);
            } else {
                if (log) alert(log);
            }

        });
        $("#RefferalCode").focus(function () {
            $("#referexist").text('');
            $("#career_save_btn").prop('disabled', false);
        });

        //------------------------------------------------------------------------------------------
        $("#career_save_btn").click(function () {
            //alert("save aa gyo");

            debugger;
            var referalCode = $("#RefferalCode").val().trim();
            if (referalCode != null && referalCode != '') {


                $.ajax({
                    url: "/Career/checkReferral",
                    type: "POST",
                    data: JSON.stringify({ 'referalcode': referalCode }),
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    success: function (data) {

                        if (data) {
                            $("#career_save_btn").prop('disabled', false);
                            $("#career_save_btn1").click();
                        }
                        else {
                            // $("#career_save_btn").prop('disabled', true);
                            $("#referexist").text("Invalid referral code ");
                        }
                    },
                    error: function () {


                    }
                });
            }
            else {
                $("#career_save_btn1").click();
            }

        });
        //--------------------------------------------------------------------------------------------------
    });
</script>


<script type="text/javascript">

    $(document).ready(function () {
        $("#OtherSpecifyFormGroup").hide();

        $("#MySuggestion1").change(function () {
            var value = document.getElementById("MySuggestion1").value;
            if (value == "JobPortal") {

                $("#HearAbtusTxtBox").val("");
                $("#HearAbtusTxtBox").attr("placeholder", "Eg. Indeed");
                $("#OtherSpecifyFormGroup").show();

            }
            if (value == "SocialMediaSites") {
                $("#HearAbtusTxtBox").attr("placeholder", "Eg. Facebook");
                $("#HearAbtusTxtBox").val("");
                $("#OtherSpecifyFormGroup").show();

            }
            if (value == "Other") {
                $("#HearAbtusTxtBox").attr("placeholder", "Please Specify");
                $("#HearAbtusTxtBox").val("");
                $("#OtherSpecifyFormGroup").show();
            }
            if (value == "JetzyCareerWebsite" || value == "SchoolUniversityCollege") {

                $("#HearAbtusTxtBox").val("NULL");

                $("#OtherSpecifyFormGroup").hide();
            }
            if (value == "") {

                $("#OtherSpecifyFormGroup").hide();
            }
        });
    })
</script>



